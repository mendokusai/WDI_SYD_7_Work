<ul>
<%= form_for @book do |f| %>
	
	<div class="form-group">
	<%= f.label :title %>
	<%= f.text_field :title %>
	</div>

	<div class="form-group">
	<%= f.label :cuisine %>
	<%= f.text_field :cuisine %>
	</div>

	<div class="form-group">
	<%= f.label :chef %>
	<%= f.text_field :chef %>
	</div>
	
	<div class="form-group">
	<%= f.label :image %>
	<%= f.url_field :image %>
	</div>
	
	<div class="form-group">
	<%= f.submit %>
	</div>
	
	

<% end %>
</ul>