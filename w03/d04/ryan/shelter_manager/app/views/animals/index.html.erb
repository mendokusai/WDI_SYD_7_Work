<table>
<tr>
<th>Name</th>
<th>Species</th>
<th>Breed</th>
<th>Age</th>
<th>Sex</th>
<th>Arrival Time</th>
<th>Departure Time</th>
<th colspan='7'></th>
</tr>
<!-- This whole table is wonky. -->
<% @animals.each do |animal | %>
<tr>
	<td><%= animal.name %></td>
	<td><%= animal.species %></td>
	<td><%= animal.breed %></td>
	<td><%= animal.age %></td>
	<td><%= animal.sex %></td>
	<td><%= animal.arrival_time %></td>
	<td><%= animal.departure_time %></td>
	<td><%= link_to animal.name, animal %></td>

	<td><%= link_to 'Show Animals', animals_path %></td>
	<td><%= link_to 'Edit Animals Details', edit_animal_path(animal) %></td>
	</tr>

<% end %>

</table>



