<ul>
	<%= form_for :animal, url: animals_path do |f| %>
		<%= f.label :name %>
		<%= f.text_field :name %><br><br>
		<%= f.label :species %>
		<%= f.text_field :species %><br><br>
		<%= f.label :breed %>
		<%= f.text_field :breed %><br><br>
		<%= f.label :arrival_time %>
		<%= f.date_field :arrival_time %><br><br>
		<%= f.label :departure_time %>
		<%= f.date_field :departure_time %><br><br>
		<%= f.label :sex %>
		<%= f.text_field :sex %><br><br>
		<%= f.label :age %>
		<%= f.number_field :age %><br><br>

		<%= f.submit %>
	<% end %>
</ul>